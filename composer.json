{
  "name": "elielelie/leafy-core",
  "description": "The Minimalistic custom framework created with Eloquent ORM.",
  "keywords": ["mvc","eloquent","php"],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "LeafyTech\\Core\\": "src/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "authors": [
    {
      "name": "Eliel Ferreira",
      "email": "eliel_elie@hotmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "PHP": "^7.4|^8.0",
    "ext-json": "*",
    "vlucas/phpdotenv": "^5.5",
    "illuminate/database": "^9.52",
    "illuminate/events": "^9.52",
    "illuminate/console": "^9.52",
    "yajra/laravel-oci8": "^9.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
